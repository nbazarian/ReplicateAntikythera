CREATE TABLE spaceevents (
    EventName text PRIMARY KEY,
    EventType text NOT NULL, 
    DateOccurs text NOT NULL,
    Speed Text NULL,
    LocationLat real NULL,
    LocationLong real NULL, 
    Directions text NULL
);

